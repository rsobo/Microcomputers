; Created: 10/15/2020 5:35:42 PM
; Author : ryans
;
;   0:0x7E, 1:0x30, 2: 0x6D, 3: 0x79, 4:0x33, 5:0x5B, 6:0x5F, 7:0x70, 8:0x7F, 9:0x7B

	RJMP Q1A
Q1A:
    LDI R16, 0xFF
	OUT DDRD, R16
	LDI R16, 0x70
	OUT PORTD, R16
END:RJMP END

Q1B:
	LDI R16, 0xFF
	OUT DDRD, R16
	LDI R16, 0x7B
	OUT PORTD, R16
END2:RJMP END2

Q2:
	LDI R16, low (RAMEND)
	OUT SPL, R16
	LDI R16, high(RAMEND)
	OUT SPH, R16
	CBI DDRC, 3
	LDI R17, 0xFF
	OUT DDRD, R20
noop:
	SBIS PINC, 3
	RJMP noop
	;0
	LDI R20, 0x7E
	OUT PORTD, R20
	CALL DELAY
	;1
	LDI R20, 0x30
	OUT PORTD, R20
	CALL DELAY
	;2
	LDI R20, 0x6D
	OUT PORTD, R20
	CALL DELAY
	;3
	LDI R20, 0x79 
	OUT PORTD, R20
	CALL DELAY
	;4
	LDI R20, 0x33 
	OUT PORTD, R20
	CALL DELAY
	;5
	LDI R20, 0x5B 
	OUT PORTD, R20
	CALL DELAY
	;6
	LDI R20, 0x5F
	OUT PORTD, R20
	CALL DELAY
	;7
	LDI R20, 0x70
	OUT PORTD, R20
	CALL DELAY
	;8
	LDI R20, 0x7F
	OUT PORTD, R20
	CALL DELAY
	;9
	LDI R20, 0x7B
	OUT PORTD, R20
	CALL DELAY
	RJMP noop
DELAY:
	LDI R28, 100
	LDI R29, 200
	LDI R30, 50
doop: 
	DEC R28
	BRNE doop
	DEC R29
	BRNE doop
	DEC R30
	BRNE doop
	RET
